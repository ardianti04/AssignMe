<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/folder"
    app:startDestination="@id/first_fragment">
    <fragment
        android:id="@+id/first_fragment"
        android:name="smt3.assignme_11.Fragment_1_home"
        android:label="Dashboard"
        tools:layout="@layout/fragment_1_home" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/second_fragment" />
    </fragment>
    <fragment
        android:id="@+id/second_fragment"
        android:name="smt3.assignme_11.timeline.Fragment_2_timeline"
        android:label="Leaderboard"
        tools:layout="@layout/fragment_2_timeline" >
        <action
            android:id="@+id/action_secondFragment_to_thirdFragment"
            app:destination="@id/third_fragment" />
    </fragment>
    <fragment
        android:id="@+id/third_fragment"
        android:name="smt3.assignme_11.Fragment_3_archive"
        android:label="Store"
        tools:layout="@layout/fragment_3_archive" >
        <action
            android:id="@+id/action_thirdFragment_to_fourthFragment"
            app:destination="@id/fourth_fragment" />
    </fragment>
    <fragment
        android:id="@+id/fourth_fragment"
        android:name="smt3.assignme_11.Fragment_4_setting"
        android:label="Profile"
        tools:layout="@layout/fragment_4_setting" />
</navigation>